{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "simple-discord-client",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "https://discord.com/app",
  "enableServer": false,
  "enableNativeAPI": false,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeAllowList": [
  ],
  "globalVariables": {
  },
  "modes": {
    "window": {
      "title": "Discord",
      "width": 1280,
      "height": 720,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "simple-discord-client",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.9.0",
    "clientVersion": "3.8.0"
  }
}